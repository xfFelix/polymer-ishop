html, body {
  margin: 0;
  padding: 0; }

.refund_container {
  width: 100%;
  height: 100%;
  background-color: #ffffff;
  border: 0.0625rem solid #DDDDDD;
  display: flex;
  flex-flow: row; }

.left_icon {
  display: flex;
  width: 14.375rem;
  height: 14.9375rem; }

.left_icon img {
  margin: auto;
  width: 5.9375rem;
  height: 5.9375rem; }

.left_title {
  margin-left: 3.8125rem;
  margin-bottom: 3.375rem;
  width: 5.875rem;
  height: 1.75rem;
  word-break: keep-all;
  font-family: PingFangTC-Light sans-serif;
  font-size: 1.25rem;
  color: #474747;
  text-align: center;
  letter-spacing: 0; }

.right_content {
  width: 23.125rem;
  height: 100%; }

.right_content .content_title {
  width: 15.8125rem;
  height: auto; }

.right_content .content_title p {
  font-size: 1.125rem;
  font-family: PingFangTC-Semibold sans-serif;
  color: #474747;
  letter-spacing: 0;
  margin-bottom: 0; }

.right_content .content_text {
  min-height: 1.25rem;
  height: auto;
  overflow: hidden;
  text-overflow: ellipsis; }

.right_content .content_text p {
  font-family: PingFangTC-Light sans-serif;
  font-size: 0.875rem;
  color: #F9A619;
  letter-spacing: 0; }

.right_content .content_text .text_shoper {
  font-family: PingFangTC-Light;
  font-size: 0.75rem;
  color: #9B9B9B;
  letter-spacing: 0; }

.right_content .content_text .text_long {
  background-color: #F5F5F5;
  height: auto;
  min-height: 3.125rem;
  width: 21.5625rem;
  font-family: PingFangTC-Light sans-serif;
  font-size: 12px;
  color: #9B9B9B;
  letter-spacing: 0; }

.right_content .content_button {
  width: 23.125rem;
  height: 3.125rem;
  display: flex;
  flex-flow: row; }

.right_content .content_button .button_ok {
  margin-top: 0.625rem;
  width: 9.0625rem;
  height: 2.5rem;
  font-family: PingFangTC-Medium;
  font-size: 1rem;
  color: #9B9B9B;
  letter-spacing: 0;
  background-color: #F5F5F5;
  line-height: 2.5rem;
  text-align: center; }

.right_content .content_button .button_link {
  margin-top: 0.625rem;
  margin-left: 3.4375rem;
  width: 9.0625rem;
  height: 2.5rem;
  font-family: PingFangTC-Medium;
  font-size: 1rem;
  color: #FFFFFF;
  letter-spacing: 0;
  background-color: #EC3437;
  line-height: 2.5rem;
  text-align: center; }

@media screen and (min-width: 360px) and (max-width: 615px) {
  .left_icon {
    width: 11.25rem; }

  .refund_container {
    height: 20.625rem; }

  .right_content .content_title p {
    margin-bottom: 0; }

  .right_content .content_button {
    margin-bottom: 1.25rem; }

  .right_content .content_button .button_link {
    margin-left: 1.25rem;
    width: 11.25rem; } }

