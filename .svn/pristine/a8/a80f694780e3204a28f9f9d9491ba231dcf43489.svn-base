    "sources": [
   "src/**/*",
   "images/**/*",
   "bower.json",
   "data/*"
  ],
    "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/**/*"
  ],
    "lint": {
        "rules": ["polymer-2"]
