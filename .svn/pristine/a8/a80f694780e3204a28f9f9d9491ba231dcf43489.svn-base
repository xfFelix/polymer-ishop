    "entrypoint": "index.html",
    "shell": "src/lt-auto/lt-app.html",
    "fragments": [
      "src/lt-auto/pages/lt-car-page.html",
      "src/lt-auto/pages/lt-home-page.html",
      "src/lt-auto/pages/lt-product-page.html",
      "src/lt-auto/pages/lt-car-detail-page.html",
      "src/lt-auto/pages/lt-model-page.html",
